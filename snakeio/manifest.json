{
    "manifest_version": 2,
    "name": "Slither.io Highscore",
    "version": "0.1.0",
    "description": "Slither.io will now remember your high score",
    "permissions": [
        "tabs",
        "*://*.slither.io/*",
        "storage"
        ],
    "content_scripts": [
        {
        "matches": ["*://*.slither.io/*"],
        "js": ["js/contentScript.js"]
         }
    ]
}   